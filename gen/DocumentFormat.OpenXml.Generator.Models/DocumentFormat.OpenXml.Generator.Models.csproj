<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <IncludeFrameworkShims>true</IncludeFrameworkShims>
    <IncludeInitShim>true</IncludeInitShim>
    <IncludeSpanShim>false</IncludeSpanShim>
    <RootNamespace>DocumentFormat.OpenXml.Generator</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" PrivateAssets="all" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="System.Collections.Immutable" />
  </ItemGroup>
</Project>